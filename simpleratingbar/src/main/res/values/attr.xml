<?xml version="1.0" encoding="utf-8"?>
<resources>
  <declare-styleable name="SimpleRatingBar">
    <attr name="borderColor" format="color" />
    <attr name="fillColor" format="color" />
    <attr name="backgroundColor" format="color" />
    <attr name="numberOfStars" format="integer" />
    <attr name="starSize" format="dimension" />
    <attr name="maxStarSize" format="dimension" />
    <attr name="starsSeparation" format="dimension" />
    <attr name="starBorderWidth" format="integer" />
    <attr name="isIndicator" format="boolean" />
    <attr name="rating" format="float" />
    <attr name="stepSize" format="float" />
    <attr name="gravity" format="enum">
      <enum name="left" value="0"/>
      <enum name="right" value="1"/>
    </attr>
  </declare-styleable>
</resources>
